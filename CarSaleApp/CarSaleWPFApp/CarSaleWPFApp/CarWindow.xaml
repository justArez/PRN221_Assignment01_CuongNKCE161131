<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarSaleWPFApp"
        xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml" x:Class="CarSaleWPFApp.CarWindow"
        mc:Ignorable="d"
        Title="CarWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnBack" Content="Back" HorizontalAlignment="Left" VerticalAlignment="Top" BorderThickness="0,0,0,0" FontFamily="Space Grotesk Medium" Background="#FFACF0F9" Margin="746,401,0,0" Click="btnBack_Click"/>
        <Button x:Name="btnReload" Content="Reload" HorizontalAlignment="Left" VerticalAlignment="Top" BorderThickness="0,0,0,0" FontFamily="Space Grotesk Medium" Background="#FFB9FFBA" Margin="683,401,0,0" Click="btnReload_Click"/>
        <DataGrid d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,46,10,45"/>
    </Grid>
</Window>
